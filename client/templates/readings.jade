.row
  .twelve.columns
    h1 Readings

    .row
      .eleven.columns
      
        .row
          .three.columns: h5 Origin
          .two.columns: h5 Time
          .seven.columns: h5 Readings

        .row(ng-repeat='reading in readings')
          .three.columns
            | {{reading.key}}
          .two.columns
            | {{reading.time | date:'MMM dd, HH:mm:ss'}}
          .seven.columns
            span(ng-repeat='(x,y) in reading | cleanup')  {{x}}: {{y}}

      .one.columns
        canvas#mybar(width='100%',height='400')
