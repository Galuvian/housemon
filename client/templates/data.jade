.row
  .twelve.columns
    h1 Locations

    .row(ng-repeat='l in locations')
      .two.columns
        b {{l.key}}
      .ten.columns
        | {{l}}

    h1 Drivers

    div(ng-repeat='d in drivers')
      h6 {{d.key}}
      .row(ng-repeat='(k,v) in d')
        .one.columns.offset-by-one
          b {{k}}
        .ten.columns
          | {{v}}
